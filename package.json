{
  "name": "postcss-px-to-px-dt-dt",
  "version": "0.0.2",
  "description": "A CSS post-processor that converts px to px",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "postcss-px-to-px-dt"
  ],
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage && eslint ."
  },
  "author": "FengXianGuo 824042624@qq.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FengXianGuo/postcss-px-to-px-dt.git"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "postcss": "^8.3.0"
  },
  "devDependencies": {
    "clean-publish": "^3.4.2",
    "eslint": "^8.0.1",
    "eslint-plugin-jest": "^25.2.2",
    "jest": "^27.3.1",
    "postcss": "^8.3.11"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:jest/recommended"
    ],
    "rules": {
      "jest/expect-expect": "off"
    }
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "bugs": {
    "url": "https://github.com/FengXianGuo/postcss-px-to-px-dt/issues"
  },
  "homepage": "https://github.com/FengXianGuo/postcss-px-to-px-dt#readme"
